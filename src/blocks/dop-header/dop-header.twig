<header class="dop-header header">
	<div class="container">
		<div class="header__row header__desctop row">
			<a href="." class="col-auto header__logo">
				<img src="img/logo-pegas-head.svg" />
			</a>
			<div class="header__lists-block col">
				<ul class="header__list justify-content-between">
					<li class="header__item header__item__social-list">
						<a href="https://www.instagram.com/pegas_orto/"><img src="img/inst.svg" /></a>
						<a href="https://ok.ru/profile/583969502001"><img src="img/odnok.svg" /></a>
						<a href="https://www.instagram.com/bereginya_blag/"><img src="img/inst.svg" /></a>
					</li>
					<li class="header__item">
						<div class="header__item-label header__item-label--dop" onclick="pegas.dopHeader.openFinder(this)">
							<input type="text" class="header__item-input">
							<img src="img/finder.svg" />
							<label for="" class="header__item-label-inner">
								<input type="text" class="header__item-input-open" placeholder="введите запрос">
								<img src="img/finder-a.svg" />
							</label>
